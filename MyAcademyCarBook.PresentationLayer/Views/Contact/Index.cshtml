
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/UILayout/Index.cshtml";
}


<div class="site-section bg-light" id="contact-section">
    <div class="container">
        <div class="row justify-content-center text-center">
            <div class="col-7 text-center mb-5">
                <h2>Araç Kiralamak İçin Bize Ulaşın Veya  Form Üzerinden Mesaj Gönderin</h2>
                <p>Mesajlarınıza en hızlı şekilde dönüş sağlamaktadır.</p>
            </div>
        </div>
        <div class="row">

            @await Html.PartialAsync("SendMessage")
          

            @await Component.InvokeAsync("_ContactInfoComponentsPartial")
            <br />
        </div>
       

        <div class="form-group row">
            <div class="col-lg-12 text-center">
                 <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d73249.21075829945!2d32.56569023411203!3d37.97638622976702!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14d09391b2de1b15%3A0x319d8add616480d9!2sKonya%20Organize%20Sanayi%20B%C3%B6lgesi!5e0!3m2!1str!2str!4v1700744667530!5m2!1str!2str" width="1100" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
        </div>
    </div>
</div>
